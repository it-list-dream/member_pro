/**app.wxss**/
page{
  background-color: #F0F2F5;
  font-family: PingFang SC;
}
::-webkit-scrollbar{
  width: 0;
  height: 0;
  color: transparent;
  }
/* 动画加载效果 */
.weui-loadmore {
  width: 100%;
  height: 100rpx;
  font-size: 30rpx;
  text-align: center;
  margin-top: 30rpx;
}

.weui-loading {
  width: 50rpx;
  height: 50rpx;
  display: inline-block;
  vertical-align: middle;
  -webkit-animation: weuiLoading 1s steps(12, end) infinite;
  animation: weuiLoading 1s steps(12, end) infinite;
  background: transparent url(data:image/svg+xml;base64,) no-repeat;
  background-size: 100%;
}

.weui-loadmore-tips {
  display: inline-block;
  vertical-align: middle;
  font-size: 28rpx;
}
/*  */
@font-face {
  font-family: "iconfont"; /* Project id 2775595 */
  src: url('//at.alicdn.com/t/font_2775595_8rjk18hefx7.woff2?t=1630217780328') format('woff2'),
       url('//at.alicdn.com/t/font_2775595_8rjk18hefx7.woff?t=1630217780328') format('woff'),
       url('//at.alicdn.com/t/font_2775595_8rjk18hefx7.ttf?t=1630217780328') format('truetype');
}

.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-sousuo:before {
  content: "\e616";
}

.icon-cancel01:before {
  content: "\e734";
}

/*  */
.weui-half-screen-dialog {
  height: 40vh !important;
}

.weui-half-screen-dialog__hd {
  min-height: 20rpx !important;
}

.weui-half-screen-dialog__bd{
  padding: 0 !important;
}

