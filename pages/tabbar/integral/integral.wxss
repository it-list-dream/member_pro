.inteTip {
  width: 100%;
  height: 210rpx;
}

/*  */
.navbar {
  position: relative;
  top: 0;
  left: 0;
  z-index: 10;
  width: 100%;
  overflow: hidden;
  flex-shrink: 0;
}

.navbar-title {
  position: absolute;
  left: 0;
  width: 100%;
  height: 32px;
  box-sizing: border-box;
  padding:0 115px;
  line-height: 32px;
  text-align: center;
  z-index: 10;
  font-size: 18px;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
  font-family: PingFang SC;
  font-weight: 500;
  color: #FFFFFF;
}

.tip {
  margin-left: 15px;
  display: flex;
  justify-content: space-around;
  align-items: center;
  width: 160rpx;
  height: 60rpx;
  background: #B4B3BB;
  border-radius: 30rpx;
}

.tipItem {
  text-align: center;
  width: 70rpx;
  line-height: 70rpx;
  height: 100%;
}

.line {
  width: 1px;
  height: 24rpx;
  background: #DDDDDD;
}

.tip image {
  width: 36rpx;
  height: 36rpx;
}



/*  */
.integral_list {
  position: relative;
  padding: 0 30rpx;
  background-color: #F0F2F5;
}

.inte_title1 {
  position: absolute;
  top: -28px;
  left: 50%;
  transform: translateX(-50%);
  display: flex;
  align-items: center;
  width: 690rpx;
  height: 136rpx;
  /* margin: 0 auto; */
  background: #ffffff;
  border-radius: 8rpx;
  font-size: 28rpx;
}

.inte_item1 {
  box-sizing: border-box;
  display: flex;
  height: 100%;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 24rpx 44rpx;
}

.inte_item1>view:first-child {
  font-size: 22rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #333333;
}

.inte_item1>view:last-child {
  font-size: 32rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #333333;
}

.inte_con {
  margin-left: 15rpx;
}

.char {
  font-size: 22rpx;
}

.line {
  width: 1px;
  height: 50rpx;
  background: rgba(0, 0, 0, 0.1);
}

.bill {
  margin: 0 40rpx;
  width: 105rpx;
  height: 39rpx;
  line-height: 39rpx;
  background: #12D58B;
  border-radius: 8rpx;
  font-size: 22rpx;
  font-family: PingFang SC;
  text-align: center;
  font-weight: 500;
  color: #FFFFFF;
}
/* 轮播图 */
.spend_img {
  padding-top: 100rpx;
}
/* 100vh*x = 690rpx * 270 */
swiper{
  width: 100vw;
  height: calc(690*270/100%);
}
.spend_img image {
  width: 690rpx;
  height: 270rpx;
  vertical-align: bottom;
}
/*  */
.nav {
  height: 84rpx;
  line-height: 84rpx;
  /* background-color: gray; */
}

.nav-title {
  font-size: 30rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: #333333;
}

/*  */
.self_aword {
  display: flex;
  padding: 20rpx;
  justify-content: space-between;
  flex-wrap: wrap;
  background: #FFFFFF;
  border-radius: 10rpx;

}

.self_item {
  width: 203rpx;
  height: 261rpx;
  background: #FFFFFF;
  box-shadow: 0 6rpx 20rpx 0 rgba(0, 0, 0, 0.06);
  border-radius: 8rpx;
}

.self_item image {
  width: 203rpx;
  height: 154rpx;
  vertical-align: bottom;
}

.card_type {
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #333333;
  padding: 12rpx 0 0 20rpx;
}

.card_count {
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: #333333;
  padding: 12rpx 0 0 20rpx;
}

.char {
  padding-left: 4rpx;
  font-weight: normal;
}

/* vip积分 */
.inte_list {
  position: relative;
  box-sizing: border-box;
  padding:20rpx;
  display: flex;
  justify-content: space-around;
  align-items: flex-end;
  flex-wrap: wrap;
  width: 690rpx;
  height: 582rpx;
  background: #ffffff;
  border-radius: 10rpx;
  align-content:space-between;
  /* margin-bottom: 30rpx; */
}
.inte_item {
  box-sizing: border-box;
  width: 204rpx;
  height: 262rpx;
  background: #FFFFFF;
  box-shadow: 0 6rpx 20rpx 0 rgba(0, 0, 0, 0.06);
  border-radius: 8rpx;
}

.inte_item view:nth-child(2) {
  padding-left: 20rpx;
  margin: 8rpx 0;
}

.inte_num {
  box-sizing: border-box;
  padding-left: 20rpx;
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: #333333;
}

.inte_num>text {
  font-size: 22rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #333333;
}

.inte_list image {
  width: 204rpx;
  height: 154rpx;
  vertical-align: bottom;
}
.null{
  height: 30rpx;
}