.bg image {
  width: 100%;
  height: 210rpx;
  vertical-align: middle;
}

.integral_list {
  position: relative;
  padding: 0 30rpx;
  height: 100vh;
  background-color: #F0F2F5;
}

.inte_title1 {
  position: absolute;
  top: -28px;
  left: 50%;
  transform: translateX(-50%);
  display: flex;
  align-items: center;
  width: 690rpx;
  height: 136rpx;
  /* margin: 0 auto; */
  background: #ffffff;
  border-radius: 8rpx;
  font-size: 28rpx;
}

.inte_item1 {
  box-sizing: border-box;
  display: flex;
  height: 100%;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 24rpx 44rpx;
}

.inte_item1>view:first-child {
  font-size: 22rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #333333;
}

.inte_item1>view:last-child {
  font-size: 32rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #333333;
}

.inte_con {
  margin-left: 15rpx;
}

.char {
  font-size: 22rpx;
}

.line {
  width: 1px;
  height: 50rpx;
  background: rgba(0, 0, 0, 0.1);
}

.bill {
  margin: 0 40rpx;
  width: 105rpx;
  height: 39rpx;
  line-height: 39rpx;
  background: #12D58B;
  border-radius: 8rpx;
  font-size: 22rpx;
  font-family: PingFang SC;
  text-align: center;
  font-weight: 500;
  color: #FFFFFF;
}

.spend_img {
  padding-top: 100rpx;
}

.spend_img image {
  width: 690rpx;
  height: 270rpx;
  vertical-align: bottom;
}

.nav {
  height: 84rpx;
  line-height: 84rpx;
  background-color: gray;
}

.nav-title {
  font-size: 30rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: #333333;
}

/*  */
.self_aword {
  display: flex;
  padding: 20rpx;
  justify-content: space-between;
  flex-wrap: wrap;
  background: #FFFFFF;
  border-radius: 10rpx;

}

.self_item {
  width: 203rpx;
  height: 261rpx;
  background: #FFFFFF;
  box-shadow: 0 6rpx 20rpx 0 rgba(0, 0, 0, 0.06);
  border-radius: 8rpx;
}

.self_item image {
  width: 203rpx;
  height: 154rpx;
  vertical-align: bottom;
}

.card_type {
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #333333;
  padding: 12rpx 0 0 20rpx;
}

.card_count {
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: #333333;
  padding: 12rpx 0 0 20rpx;
}

.char {
  padding-left: 4rpx;
  font-weight: normal;
}
/*  */
.inte_list {
  display: flex;
  justify-content: space-around;
  flex-wrap: wrap;
  background: #FFFFFF;
  border-radius: 10px;
  /* 
  width: 690px;
height: 582px;
  */
}

.inte_item {
  margin: 20rpx 0;
  width: 204rpx;
  height: 262rpx;
  background: #FFFFFF;
  box-shadow: 0 6rpx 20rpx 0 rgba(0, 0, 0, 0.06);
  border-radius: 8rpx;
}

.inte_item view:nth-child(2) {
  padding-left: 20rpx;
  margin: 8rpx 0;
}

.inte_num {
  box-sizing: border-box;
  padding-left: 20rpx;
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: #333333;

}

.inte_num>text {
  font-size: 22rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #333333;
}
.inte_list image {
  width: 204rpx;
  height: 154rpx;
  vertical-align: bottom;
}