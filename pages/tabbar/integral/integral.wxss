/*  */
.navbar {
  position: relative;
  top: 0;
  left: 0;
  z-index: 10;
  width: 100%;
  overflow: hidden;
  flex-shrink: 0;
}

.navbar-title {
  position: fixed;
  left: 0;
  width: 100%;
  height: 32px;
  box-sizing: border-box;
  padding: 0 115px;
  line-height: 32px;
  text-align: center;
  z-index: 100;
  font-size: 18px;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
  font-family: PingFang SC;
  font-weight: 500;
  color: #FFFFFF;
}

.tip {
  position: fixed;
  display: flex;
  justify-content: space-around;
  align-items: center;
  width: 160rpx;
  height: 60rpx;
  /* background: rgba(0, 0, 0, 0.4); */
  background: rgba(203, 187, 181, 0.6);
  /* background: #B4B3BB; */
  border-radius: 30rpx;
  z-index: 200;
}

.tipItem {
  display: flex;
  align-items: center;
  justify-content: center;
  text-align: center;
  width: 70rpx;
  line-height: 70rpx;
  height: 100%;
  box-sizing: border-box;
}

.line {
  width: 1px;
  height: 24rpx;
  background: #DDDDDD;
}

.tip image {
  width: 36rpx;
  height: 36rpx;
}

/*  积分标题*/
.integral_list {
  position: relative;
  box-sizing: border-box;
  width: 100%;
  height: auto;
  padding: 0 30rpx 30rpx 30rpx;
}

.inteTitleBox {
  display: flex;
  width: 690rpx;
  height: 100rpx;
  /* background: pink; */
  background: #ffffff;
  margin-top: -48rpx;
  border-radius: 8rpx;
}

.inteTitleBox>view {
  display: flex;
  flex-direction: column;
  height: 100%;
  align-items: center;
  justify-content: center;
}

.inteTitleBox>view:nth-child(-n+2)>view:last-child {
  font-size: 32rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #333333;
}

.inteTitleBox>view:nth-child(-n+3) {
  position: relative;
  width: 28.5%;
  height: 100%;
  /* background-color: yellow; */

}

.inteTitleBox>view:nth-child(-n+3)::after {
  position: absolute;
  top: 24rpx;
  right: 0;
  display: block;
  content: '';
  width: 1px;
  height: 50rpx;
  background: #000000;
  opacity: 0.1;

}

.inteIcon {
  width: calc(100vw - 28.5% *3);
  height: 100%;
  /* background: purple; */

}

.inteIcon image {
  width: 40rpx;
  height: 40rpx;
  vertical-align: bottom;
}

.integral_bar {
  font-size: 22rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #333333;
}

.billBox .bill {
  width: 105rpx;
  height: 40rpx;
  line-height: 40rpx;
  text-align: center;
  background: #12D58B;
  border-radius: 8rpx;
  font-size: 22rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #FFFFFF;
}

.branch {
  font-size: 22rpx;
  font-family: PingFang SC;
}

