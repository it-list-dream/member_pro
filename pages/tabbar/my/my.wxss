/* pages/my/my.wxss */
.my_bg image {
  width: 100%;
  height: 290rpx;
  vertical-align: bottom;
}

.my_container {
  position: relative;
  background-color: #f0f2f5;
  height: calc(100vh - 60px);
}

.my_banner {
  position: absolute;
  top: -120rpx;
  left: 50%;
  transform: translateX(-50%);
  width: 690rpx;
  height: 230rpx;
  box-sizing: border-box;
  padding: 20rpx;
  background: #FFFFFF;
  border-radius: 8rpx;
}

.my_info {
  display: flex;
}

.per_info {
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  margin-left: 22rpx;
}

.member_info {
  font-size: 30rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #333333;
}

.member_info image {
  width: 36rpx;
  height: 36rpx;
  vertical-align: middle;
}

.head {
  width: 88rpx;
  height: 88rpx;
}

.phone {
  width: 128rpx;
  height: 26rpx;
  line-height: 26rpx;
  text-align: center;
  border: 1px solid #333333;
  border-radius: 14rpx;
  font-size: 18rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #333333;
}

.my_course {
  display: flex;
  align-items: center;
  margin-top: 32rpx;
  height: 88rpx;
  /* background-color: pink; */
  text-align: center;
}

.cItem {
  position: relative;
  flex: 1;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  font-size: 24rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #333333;
  /* text-align: center; */
}

.cItem:nth-child(2)::before {
  position: absolute;
  content: "";
  width: 2rpx;
  height: 36rpx;
  top: 18rpx;
  left: 0;
  background: #EEEEEE;
}

.cItem:nth-child(2)::after {
  position: absolute;
  content: "";
  top: 22rpx;
  right: 0;
  width: 2rpx;
  height: 36rpx;
  background: #EEEEEE;
}

/*  */
.nav {
  padding-top: 148rpx;
  padding: 148rpx 30rpx 20rpx 30rpx;
}

.nav .nva-title {
  font-size: 30rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: #333333;
}

/*  */
.active {
  padding: 0 30rpx;
  display: flex;
  justify-content: space-between;
}

.avtive_item {
  display: flex;
  align-items: center;
  justify-content: center;
  background-size: 100% 100%;
  width: 335rpx;
  height: 110rpx;
  line-height: 110rpx;
  text-align: center;
}

.leftStored {
  background-image: url(data:image/jpg;base64,);
}

.rightStored {
  background-image: url(data:image/jpg;base64,);
}

.nav1 {
  padding: 30rpx;
  font-size: 30rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: #333333;
}

.leftInte {
  background-image: url(data:image/jpg;base64,);
}

.rightInte {
  background-image: url(data:image/jpg;base64,);
}

.avtive_item .inte_con {
  height: 100%;
  display: flex;
  align-items: center;
  box-sizing: border-box;
  font-size: 20rpx;
  font-family: PingFang SC;
  color: #FFFFFF;
}

.avtive_item .inte_con text:first-child {
  font-size: 24rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: #FFFFFF;
}

.avtive_item .inte_con text:last-child {
  padding-left: 20rpx;
  font-size: 36rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: #FFFFFF;
}

/*  */
.nav2 {
  margin-top: 30rpx;
  padding: 20rpx 30rpx;
}

/* 更多服务 */
.more{
  padding: 30rpx;
}
.moreService {
  box-sizing: border-box;
  display: flex;
  align-items: center;
  flex-wrap: wrap;
  width: 690rpx;
  height: 250rpx;
  background: #FFFFFF;
  box-shadow: 0 6rpx 20rpx 0 rgba(0, 0, 0, 0.06);
  border-radius: 8rpx;
}
.service{
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  width: 172rpx;
  height: 100rpx;
  /* background-color: pink; */
}
.service image {
  width: 40rpx;
  height: 40rpx;
}
.service text{
  font-size: 24rpx;
font-family: PingFang SC;
font-weight: 500;
color: #666666;
}