/* pages/sport/sport.wxss */
.sprotTip {
  position: relative;
  width: 100%;
  height: 280rpx;
}

.bg_image {
  position: absolute;
  z-index: -1;
  left: 0px;
  top: 0px;
  width: 100%;
  height: 100%;
}

/*  */
.navbar {
  position: relative;
  top: 0;
  left: 0;
  z-index: 10;
  width: 100%;
  overflow: hidden;
  flex-shrink: 0;
}

.navbar-title {
  position: fixed;
  left: 0;
  width: 100%;
  height: 32px;
  box-sizing: border-box;
  padding: 0 115px;
  line-height: 32px;
  text-align: center;
  z-index: 100;
  font-size: 18px;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
  font-family: PingFang SC;
  font-weight: 500;
  color: #FFFFFF;
}

.tip {
  position: fixed;
  display: flex;
  box-sizing: border-box;
  justify-content: space-around;
  align-items: center;
  width: 160rpx;
  height: 60rpx;
  /* background: #B4B3BB; */
  /* background: rgba(0, 0, 0, 0.4); */
  background: rgba(203, 187, 181, 0.6);
  border-radius: 30rpx;
  z-index: 200;
}

.tipItem {
  display: flex;
  align-items: center;
  justify-content: center;
  text-align: center;
  width: 70rpx;
  line-height: 70rpx;
  height: 100%;
  box-sizing: border-box;
}

.line {
  width: 1px;
  height: 24rpx;
  background: #DDDDDD;
}

.tip image {
  width: 36rpx;
  height: 36rpx;
}

/*  */
/* page{
  padding-bottom: 48px;
} */
.all {
  position: relative;
  padding: 0 30rpx 130rpx 30rpx;
}

/*  */
.sport_device {
  position: absolute;
  top: -36px;
  left: 0;
  width: 100%;
  height: 172rpx;
}

.device_wrapper {
  padding: 0 30rpx;
  display: flex;
  justify-content: space-between;
}

.sport_tip {
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0 18rpx;
  width: 330rpx;
  height: 172rpx;
  background: linear-gradient(-30deg, #93B3EA, #87A7E0);
  border-radius: 8rpx;
}

.fitness {
  background: linear-gradient(-30deg, #9997D8, #877FD3);
}

.btn {
  margin-top: 4rpx;
  width: 104rpx;
  height: 32rpx;
  line-height: 32rpx;
  text-align: center;
  background: #FFFFFF;
  border-radius: 16rpx;
  font-size: 22rpx;
  font-family: PingFang SC;
  font-weight: 500;
}

.go_run {
  color: #90B0E7;
}

.to_dete {
  color: #9997D8;
}

.sport_day view:first-child {
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #FFFFFF;
}

.sport_day view:nth-child(2) {
  /* margin: 20rpx 0; */
  font-size: 48rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: #FFFFFF;
}

.test {
  font-size: 22rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #FFFFFF;
}

/*  */
.run {
  /* padding-top:100rpx; */
  padding-top: 124rpx !important;
}

.nav {
  padding: 22rpx 0;
  display: flex;
  justify-content: space-between;
  align-items: center;
  /* background-color: pink; */
}

.nav-title {
  font-size: 34rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: #333333;
}

.nav-name {
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: #333333;
  margin-right: 10rpx;
}

.nav-img {
  /* width: 8rpx;
  height: 14rpx; */
  width: 24rpx;
  height: 24rpx;
}

.sp_calendar {
  width: 690rpx;
  height: auto;
  background: #FFFEFF;
  border-radius: 10rpx;
}

/* 日历 */
.calendar_wrapper {
  height: 100rpx;
  /* background-color: pink; */
}

.calendar_item {
  display: flex;
  height: 100%;
}

.calendar_item li {
  flex: 1;
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  align-items: center;
}

.calendar_item li view {
  font-size: 22rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #999999;
}

.change_active {
  width: 36rpx;
  height: 36rpx;
  line-height: 36rpx;
  text-align: center;
  background: #12D58B;
  border-radius: 50%;
  font-size: 18rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: #FFFEFF !important;
}

/*  */
.calendar_tool {
  /* position: relative; */
  display: flex;
  height: 120rpx;
  margin: 0 20rpx;
  padding: 24rpx 0;
  border-bottom: 1px solid #E5E5E5;
  justify-content: space-between;
}

.calendar_tool:last-child {
  border: none;
}

.calendar_tool text {
  padding-left: 10rpx;
}

.per_course {
  display: flex;
}

.per_title {
  width: 97rpx;
  height: 105rpx;
  background: #52495B;
  border-radius: 8rpx;
  text-align: center;
  line-height: 105rpx;
  font-size: 60rpx;
  font-family: PingFang SC;
  font-weight: 800;
  color: #FFFEFF;
}

.per_item {
  display: flex;
  height: 100%;
  flex-direction: column;
  justify-content: space-around;
  margin-left: 18rpx;
}

.calendar_tool .icon {
  width: 36rpx;
  height: 36rpx;
  vertical-align: middle;
  margin-top: -4rpx;
}

.per_course_left {
  display: flex;
  align-items: center;
}

.per_course_right {
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  height: 100%;
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #333333;
}

.per_item>view>text {
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #333333;
}

/*  */
.con {
  overflow: hidden;
  /* background-color: yellowgreen; */
  padding: 10rpx 0 24rpx 0;
  border-radius: 0 0 10rpx 10rpx;
}

.no_appointment {
  height: 303rpx;
  overflow: hidden;
  border-radius: 0 0 10rpx 10rpx;
  /* background: red; */
}

.app_tip {
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #999999;
}

.no_appointment>view {
  margin-top: 101rpx;
  text-align: center;
  width: 100%;
  /* display: flex; */
  /* flex-direction: column;
  align-items: center;
  justify-content: center; */
}

.no_appointment image {
  width: 36rpx;
  height: 36rpx;
  vertical-align: middle;
}

.appointment_btn {
  margin: 0 auto;
  margin-top: 50rpx;
  width: 214rpx;
  height: 52rpx;
  line-height: 52rpx;
  text-align: center;
  font-size: 30rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #FFFFFF;
  background: #12D58B;
  border-radius: 26rpx;
}

.appointment_btn image {
  margin-top: -6rpx;
}

/* 自律积分 */
.inte_list {
  box-sizing: border-box;
  padding: 20rpx;
  /* height: 584rpx; */
  height: auto;
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
  width: 690rpx;
  background: #ffffff;
  border-radius: 10rpx;
}

.inte_item {
  box-sizing: border-box;
  width: 204rpx;
  height: 262rpx;
  background: #FFFFFF;
  box-shadow: 0 6rpx 20rpx 0 rgba(0, 0, 0, 0.06);
  border-radius: 8rpx;
  margin-bottom: 20rpx;
}

.inte_item>view:first-child>image {
  border-radius: 8rpx 8rpx 0 0;
}

.inte_item>view:nth-child(2) {
  /* font-size: 28rpx; */
  font-family: PingFang SC;
  font-weight: 500;
  color: #333333;
}

.inte_item:not(:nth-child(3n)) {
  margin-bottom: 0 !important;
}

.inte_item:last-child {
  margin-bottom: 0;
}

.inte_item::after {
  content: '';
  width: 204rpx;
}

/* 
.inte_item view:nth-child(2) {
  padding-left: 20rpx;
  margin: 1rpx 0;
} */

.inte_bottom {
  box-sizing: border-box;
  display: flex;
  flex-direction: column;
  height: 107rpx;
  padding: 15rpx 0;
  justify-content: space-between;
}

.inte_bottom>view {
  margin-left: 20rpx;
}

.inte_bottom>view:first-child {
  font-size: 28rpx;
}

.inte_num {
  box-sizing: border-box;
  /* padding-left: 20rpx; */
  font-size: 26rpx;
  font-family: PingFang SC;
  /* font-weight: bold; */
  color: #333333;
}

.inte_num>text {
  font-size: 22rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #333333;
}

.inte_list image {
  width: 204rpx;
  height: 154rpx;
  vertical-align: bottom;
}

/* 蒙层 */
.mask {
  position: fixed;
  z-index: 99;
  height: 100%;
  width: 100%;
  top: 0;
  left: 0;
  /* background: #000000;
  opacity: 0.6; */
  background: rgba(0, 0, 0, .6);
}

.modalDlg {
  position: absolute;
  left: 0;
  bottom: 0;
  width: 100%;
  /* height: 400rpx; */
  height: auto;
  z-index: 120;
  background: #FFFFFF;
  border-radius: 8rpx 8rpx 0 0;
  font-size: 40rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #333333;
}

.modalDlg_item {
  padding: 0 30rpx;
}

.mask_title {
  display: flex;
  justify-content: space-between;
  margin: 24rpx 0;
  font-size: 35rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #333333;
  font-weight: 500;
}

.mask_title cover-image {
  width: 36rpx;
  height: 36rpx;
}

.sa_course {
  box-sizing: border-box;
  display: flex;
  /* height: 68rpx; */
  padding: 20rpx 0;
  justify-content: space-between;
  font-size: 30rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #333333;
  align-items: flex-end;
  border-bottom: 1px solid rgba(0, 0, 0, 0.2);
}


.sa_course cover-image {
  width: 36rpx;
  height: 36rpx;
}
.close{
  position: fixed;
  z-index: 2;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
}
/*  */
.none{
  display: flex;
  height: 40vh;
  align-items: center;
  justify-content: center;
  text-align: center;
  background: #ffffff;
}
.none_list image{
  width: 180rpx;
  height: 200rpx;
}