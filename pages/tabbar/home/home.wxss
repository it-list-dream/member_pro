.banner {
  position: relative;
}

swiper {
  width: 100%;
  height: calc(100vw * 478 / 750);
}

swiper image {
  width: 100%;
  height: calc(100vw * 478 / 750);
}

.head_wrapper {
  position: fixed;
  width: 233rpx;
  height: 60rpx;
  /* background: rgba(0, 0, 0, 0.4); */
  background: rgba(203, 187, 181,0.6);
  /* background: #F0F2F5;
  opacity: 0.6; */
  border-radius: 30rpx;
  z-index: 100;
}

.head_wrapper .wrapper_item {
  height: 100%;
  display: flex;
  justify-content: space-evenly;
  align-items: center;
}

.header_list {
  height: 100%;
  padding: 0 15rpx;
  display: flex;
  align-items: center;
}

.head_wrapper .line {
  width: 1rpx;
  height: 24rpx;
  background: #DDDDDD;
}

.head_wrapper .head_pic {
  width: 30rpx;
  height: 30rpx;
}

/* 内容 */
.home_content {
  position: relative;
  height: auto;
  padding-bottom: 30rpx;
}

.store_pic {
  position: absolute;
  top: -63rpx;
  left: 36rpx;
}

.store_pic image {
  width: 130rpx;
  height: 130rpx;
  border-radius: 50%;
}

.store_wrapper {
  display: flex;
  flex-direction: column;
  height: 120rpx;
  padding-left: 183rpx;
  background-color: #ffffff;
}

.store_wrapper .store_title {
  margin-top: 21rpx;
  font-size: 30rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #333333;
}

.store_wrapper .store_bottom {
  margin-top: 10rpx;
  display: flex;
}

.store_wrapper .do_business {
  height: 34rpx;
  line-height: 34rpx;
  opacity: 0.6;
  border-radius: 17rpx;
  font-size: 24rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #12D58B;
}

.store_wrapper .do_business .time {
  padding-left: 9rpx;
}

.store_wrapper .information {
  display: flex;
  padding-left: 111rpx;
  height: 40rpx;
  box-sizing: border-box;
  line-height: 40rpx;
  text-align: center;
  border-radius: 20rpx;
}

.store_wrapper .information .phone_wrapper {
  width: 36rpx;
  height: 36rpx;
  text-align: center;
  line-height: 36rpx;
  background: #12D58B;
  border-radius: 50%;
}

.store_wrapper .information .phone_info {
  padding-left: 9rpx;
  font-size: 24rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #12D58B;
}

.store_wrapper .phone {
  width: 23rpx;
  height: 23rpx;
}

/* 课程列表 */
.classList {
  display: flex;
  margin-top: 20rpx;
  height: 190rpx;
  background-color: #FFFFFF;
}

.classList .classList_item {
  padding: 20rpx 0;
  box-sizing: border-box;
  flex: 1;
  display: flex;
  height: 100%;
  flex-direction: column;
  justify-content: space-between;
  align-items: center;
}

.classList .classList_item text {
  /* margin-top: 10rpx; */
  font-size: 30rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: #333333;
}

.classList_item .class_pic {
  width: 100rpx;
  height: 100rpx;
}

/* 活动 */
.coach_list {
  padding: 24rpx 30rpx;
  display: flex;
  justify-content: space-between;
  font-family: PingFang SC;
  background: #F0F2F5;
}

.coach_list .star_coach {
  font-size: 34rpx;
  font-weight: bold;
  color: #333333;
}

.coach_list .all_t text {
  padding-right: 5rpx;
  font-size: 28rpx;
  font-weight: 400;
  color: #333333;
  vertical-align: middle;
}

.coach_list .all_t image {
  width: 24rpx;
  height: 24rpx;
  vertical-align: middle;
}

/* 明星教练 */
.starCoachList {
  padding: 0 30rpx;
}

.star_item {
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
  padding: 20rpx;
  width: 690rpx;
  height: 335rpx;
  background: #FFFFFF;
  border-radius: 8rpx;
}

/* coach */
.coach_wrapper {
  box-sizing: border-box;
  width: 204rpx;
  height: 295rpx;
  background: #FFFFFF;
  box-shadow: 0px 6rpx 20rpx 0px rgba(0, 0, 0, 0.06);
  border-radius: 8rpx;
  /* background: green; */
}

/* .top_star {
  width: 204rpx;
  height: 148rpx;

} */

.top_star {
  width: 204rpx;
  height: 148rpx;
  overflow: hidden;
}

.top_star_img {
  width: 100%;
  height: 100%;
  border-radius: 8rpx 8rpx 0  0 !important;
}

.bottom_star {
  box-sizing: border-box;
  padding: 0 20rpx;
  width: 100%;
  height: 188rpx;
}

.star_title {
  margin-bottom: 10rpx;
}

.coachInfo1 {
  display: flex;
  padding-top: 6rpx;
}

.star_title text {
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #333333;
}

