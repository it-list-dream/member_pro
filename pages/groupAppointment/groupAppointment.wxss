.price_box {
  display: flex;
  justify-content: space-between;
  height: 120rpx;
  align-items: center;
}

.price {
  font-size: 50rpx;
  font-family: Alibaba PuHuiTi;
  font-weight: bold;
  color: #FD575C;
}

.price_box .appoinmentBtn {
  width: 220rpx;
  height: 80rpx;
  margin: 0;
  padding: 0;
  line-height: 80rpx;
  text-align: center;
  background: #52495B;
  border-radius: 40rpx;
  font-size: 30rpx;
  color: #FFFFFF !important;
}
button::after { border: none; }

.free_group {
  display: flex;
  align-items: center;
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 140rpx;
  padding: 0 30rpx;
  box-sizing: border-box;
  background-color: #ffffff;
  box-shadow: 0px 0px 8rpx 0px rgba(4, 0, 0, 0.26);
}

.free_group .appoinemnt {
  width: 690rpx;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  margin: 0;
  padding: 0;
  background: #52495B;
  border-radius: 50rpx;
  font-size: 30rpx;
  font-family: PingFang SC;
  color: #FFFFFF !important;
}

/* 新的样式 */
/*  */
/* 选座 */
.mask {
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  z-index: 1000;
  background: rgba(0, 0, 0, 0.6);
}

.mask-item {
  position: absolute;
  background-color: #ffffff;
  bottom: 0;
  left: 0;
  width: 100%;
  height: auto;
  padding-bottom: 30rpx;
  z-index: 99;

}

.seatTitle {
  height: 68rpx;
  line-height: 68rpx;
  font-size: 30rpx;
  font-family: PingFang SC;
  /* font-weight: 500; */
  color: #333333;
  border-bottom: 1px solid #eeeeee;
}

.seatAll {
  display: flex;
  flex-wrap: wrap;
  padding: 30rpx 0;
}

.seatAll li {
  flex-basis: 10%;
}

.seat {
  margin: 0 auto;
  width: 48rpx;
  height: 48rpx;
  line-height: 48rpx;
  text-align: center;
  border: 1px solid #999999;
  border-radius: 6rpx;
  margin-bottom: 20rpx;
  font-size: 26rpx;
  font-family: PingFang SC;
  /* font-weight: 500; */
  color: #999999;
}

.confirm-btn {
  margin: 20rpx auto;
  width: 220rpx;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  border-radius: 12rpx;
  background: #12D58B;
  color: #ffffff;
  font-size: 32rpx;
  font-family: PingFang SC;
}

.close {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 2;
}

.col {
  background: #FF4349;
  border: none !important;
  color: #ffffff;
}

.choosing {
  color: #ffffff;
  background: #12D58B;
  border: none;
}