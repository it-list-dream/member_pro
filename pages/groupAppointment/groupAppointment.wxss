page {
  font-family: PingFang SC;
  background-color: #FFFFFF;
}

.group_container {
  position: relative;
  background-color: #F9F9F9;
}

/* 图片 */
.groupImage {
  position: relative;
  width: 690rpx;
  margin: 0 auto;
  height: 388rpx;
  background-color: #ffffff;
  box-sizing: border-box;
  overflow: hidden;
  border-radius: 10rpx;
}

.mask-bg {
  position: absolute;
  top: 0;
  left: 0;
  width: 690rpx;
  height: 388rpx;
  background:rgba(0, 0,0, 0.3);
  z-index: 10;
}

.groupImage image {
  position: absolute;
  top: 0;
  left: 50%;
  transform: translateX(-50%);
  height: 388rpx;
  /* vertical-align: bottom; */
  border-radius: 10rpx;
  z-index: 2;
}

.attentNum {
  position: absolute;
  bottom: 30rpx;
  right: 30rpx;
  padding: 0 16rpx;
  text-align: center;
  height: 50rpx;
  line-height: 50rpx;
  border-radius: 8rpx;
  font-size: 22rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: #FFFFFF;
  z-index: 100;
}

/*  */
.coach_detail {
  display: flex;
  justify-content: space-between;
  padding: 40rpx 30rpx;
  box-sizing: border-box;
  background-color: #ffffff;
}

.left_box {
  display: flex;
}

.left_box .head {
  width: 102rpx;
  height: 102rpx;
  border-radius: 50%;
}

.coach_box {
  padding: 10rpx 0 0 20rpx;
}

.coach_box text {
  padding-right: 20rpx;
  font-size: 30rpx;
  font-weight: bold;
  color: #000000;
}

.coach_box image {
  width: 30rpx;
  height: 30rpx;
  vertical-align: middle;
}

.right_box {
  /* align-self: flex-end; */
  font-size: 22rpx;
  color: #B6B9BE;
}

.right_box>view:last-child {
  margin-top: 10rpx;
}

.group_detail {
  margin-top: 20rpx;
  padding: 0 30rpx;
  background-color: #ffffff;
  box-sizing: border-box;
}

.group_col {
  width: 100%;
  height: 106rpx;
  line-height: 106rpx;
  border-bottom: 0.5px solid #e5e5e5;
}

.group_col:last-child {
  border: none !important;
}

.group_icon {
  width: 32rpx;
  height: 32rpx;
  vertical-align: middle;
}

.group_col text {
  padding-left: 15rpx;
  font-size: 28rpx;
  color: #000000;
}

.gruop_introduce {
  margin-top: 20rpx;
  padding: 60rpx 30rpx;
  background-color: #ffffff;
}

.introduce_title {
  display: flex;
  align-items: center;
  font-size: 28rpx;
  font-weight: 800;
  color: #000000;
}

.introduce_title::before {
  content: '';
  display: inline-block;
  width: 20rpx;
  height: 20rpx;
  border: 2rpx solid #12D58B;
  border-radius: 50%;
}

.introduce_title text {
  padding-left: 14rpx;
  font-size: 30rpx;
  font-weight: 800;
  color: #000000;
}

.introduce_content {
  padding: 40rpx 0 220rpx;
  word-break: break-all;
  word-wrap: break-word;
  font-size: 26rpx;
  color: #000000;
  line-height: 48rpx;
}

.group_bottom {
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 240rpx;
  padding: 0 30rpx;
  box-sizing: border-box;
  background-color: #ffffff;
  box-shadow: 0px 0px 8rpx 0px rgba(4, 0, 0, 0.26);
}

.wechatPay {
  display: flex;
  height: 118rpx;
  align-items: center;
  border-bottom: 1px solid #e5e5e5;
}

.wechatPay image {
  width: 30rpx;
  height: 30rpx;
}

.wechatPay text {
  padding-left: 18rpx;
  font-size: 28rpx;
  font-weight: 800;
  color: #000000;
}

