.group_container {
  position: relative;
  background-color: #F0F2F5;
}

/* 图片 */
.groupImage {
  position: relative;
  width: 100%;
  height: 421rpx;
}

.groupImage image {
  width: 100%;
  height: 100%;
  vertical-align: bottom;
}

.attentNum {
  position: absolute;
  bottom: 30rpx;
  right: 30rpx;
  padding: 0 16rpx;
  text-align: center;
  height: 50rpx;
  line-height: 50rpx;
  /* background: #7B6F85; */
  border-radius: 8rpx;
  font-size: 22rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: #FFFFFF;
}

/*  */
.course_date {
  padding: 0 30rpx;
  display: flex;
  justify-content: space-between;
  height: 130rpx;
  background-color: #FFFFFF;
  border: 1px solid #eeeeee;
  box-sizing: border-box;
}

.course_date .left {
  display: flex;
  align-items: center;
}

.course_date .left image {
  vertical-align: middle;
}

.course_date .left image:first-child {
  width: 53rpx;
  height: 53rpx;
  border-radius: 50%;
}

.course_date .left image:last-child {
  width: 40rpx;
  height: 40rpx;

}

.column {
  padding: 30rpx;
  font-size: 30rpx;
  font-family: PingFang SC;
  color: #333333;
  background: #FFFFFF;
}



.course_date .coachName {
  padding: 0 9rpx 0 20rpx;
  font-size: 48rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: #333333;
}



.course_date .right {
  display: flex;
  height: 100%;
  flex-direction: column;
  /* justify-content: space-evenly; */
  font-size: 24rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #B6B9BE;
}


.course_name {
  border: 1px solid #eeeeee;
}

.course_time {
  text-indent: 1em;
  border: 1px solid #eeeeee;
}

.course_address {
  text-indent: 1em;
  border-bottom: 1px solid #eeeeee;
}

.course_introduce {
  display: flex;
  font-weight: 400;
  color: #333333;
  background: #FFFFFF;
  text-indent: 1em;
}

.course_introduce .introduce {
  white-space: nowrap;
}

/* 免费的团课 */
.order_list {
  position: fixed;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 100rpx;
  background: #FFFFFF;
}

.order {
  width: 690rpx;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  background: #12D58B;
  border-radius: 30rpx;
  color: #FFFFFF;
  margin: 0 auto;
  margin-top: 20rpx;
}

/* 支付方式 */
.pay_course {
  position: fixed;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 212rpx;
  background-color: #FFFFFF;
}

.pay_course .pay_list {
  padding: 0 34rpx;
}

.pay_course .pay_list .pay {
  display: flex;
  align-items: center;
  /* height: 102rpx; */
  padding: 26rpx 0;
  border-bottom: 1px solid #eeeeee;
}

.pay_course .pay_title {
  font-size: 36rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #333333;
}

.pay_course .WeChatPay {
  display: flex;
  justify-content: center;
  align-items: center;
  margin-left: 22rpx;
  width: 201rpx;
  height: 55rpx;
  border: 1px solid #cccccc;
  font-size: 30rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #333333;

}

.pay_course .WeChatPay image {
  width: 36rpx;
  height: 36rpx;
  padding-left: 28rpx;
  vertical-align: middle;
}

.pay_course .pay_total {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 26rpx 0;
  font-size: 36rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #333333;
}

.pay_course .pay_total .pay_number {
  display: flex;
}

.pay_course .pay_total .pay_number .price {
  color: red;
}

.pay_order .order_title {
  width: 160rpx;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  color: #ffffff;
  font-size: 34rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #FFFFFF;
  background: #12D58B;
  border-radius: 30rpx;
}

/* 选座 */
.mask {
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  z-index: 1000;
  background: rgba(0, 0, 0, 0.6);
}

.mask-item {
  position: absolute;
  background-color: #ffffff;
  bottom: 0;
  left: 0;
  width: 100%;
  height: auto;
  padding-bottom: 30rpx;
  z-index: 99;

}

.seatTitle {
  height: 68rpx;
  line-height: 68rpx;
  font-size: 30rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #333333;
  border-bottom: 1px solid #eeeeee;
}

.seatAll {
  display: flex;
  flex-wrap: wrap;
  padding: 30rpx 0;
}

.seatAll li {
  flex-basis: 10%;
}

.seat {
  margin: 0 auto;
  width: 48rpx;
  height: 48rpx;
  line-height: 48rpx;
  text-align: center;
  border: 1px solid #999999;
  border-radius: 6rpx;
  margin-bottom: 20rpx;
  font-size: 26rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #999999;
}

.confirm-btn {
  margin: 20rpx auto;
  width: 220rpx;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  border-radius: 12rpx;
  background: #12D58B;
  color: #ffffff;
  font-size: 32rpx;
  font-family: PingFang SC;
}

.close {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 2;
}

.col {
  background: #FF4349;
  border: none !important;
  color: #ffffff;
}

.choosing {
  color: #ffffff;
  background: #12D58B;
  border: none;
}