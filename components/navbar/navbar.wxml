<view class="navbar custom-class" style='height:{{navHeight}}px;background-color:{{bgColor}}'>
  <!-- <view style="position:fixed; top:0;left:0;width: 100%;z-index:2"> -->
    <view wx:if="{{showNav}}" class="navbar-action-wrap navbar-action-group row item-center"
      style='top:{{navTop}}px;background-color:rgba(255,255,255,.6)' bindtap="_navBack">
      <view class="icon">
        <image src="/static/arrow.png" class="icon-arrow"></image>
      </view>
    </view>
    <view class='navbar-title' style='top:{{navTop}}px'>
      {{pageName}}
    </view>
  <!-- </view> -->
</view>