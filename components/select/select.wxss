.com-selectBox{
  width: 168rpx;
}
.com-sContent{
  position: relative;
  height: 50rpx;
  line-height: 50rpx;
  border: 1px solid #e2e2e2;
  background: white;
  font-size: 28rpx;
}
.com-sImg{
  position: absolute;
  top: 50%;
  right: 10rpx;
  width: 28rpx;
  height: 28rpx;
  transform: translateY(-50%);
}
.com-sTxt{
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  padding:0 40rpx 0 12rpx;
  font-size: 28rpx;
}
.com-sList{
  background: #F0F2F5;
  width: inherit;
  position: absolute;
  border: 1px solid #e2e2e2;
  border-top: none;
  box-sizing: border-box;
  z-index: 3;
  max-height: 160rpx;
  overflow: auto;
}
.com-sItem{
  box-sizing: border-box;
  height: 50rpx;
  line-height: 50rpx;
  border-top: 1px solid #e2e2e2;
  padding: 0 20rpx;
  text-align: left;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  font-size: 28rpx;
  
}
.changeList{
   background: #A4A6A7;
}
.com-sItem:first-child{
  border-top: none;
}