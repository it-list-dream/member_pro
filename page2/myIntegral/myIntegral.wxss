/* 内容区 */
::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}

.container {
  background: #ffff;
}

/*  */
.inte_tabs {
  display: flex;
  align-items: center;
  position: relative;
  width: 100%;
  height: 100rpx;
  font-family: PingFang SC;
  box-sizing: border-box;
  padding: 0 30rpx;
}


.tabs_item {
  position: relative;
  width: 25%;
  text-align: center;
}

.tabs_item>text {
  color: #999999;
  font-size: 32rpx;
}

select {
  display: block;
}

.active text {
  color: #222222;
}

.active::after {
  position: absolute;
  bottom: -12rpx;
  left: 50%;
  transform: translateX(-50%);
  content: '';
  width: 20rpx;
  height: 6rpx;
  background: #52495B;
  border-radius: 2rpx;
}


/*  */
.record {
  background-color: white;
}

::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}

.record_item {
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin: 0 auto;
  padding-left: 15rpx;
  overflow: hidden;
  width: 690rpx;
  height: 120rpx;
  border-bottom: 1px solid #eeeeee;
  box-sizing: border-box;
}

.record_item:last-child {
  border: none !important;
}

.left_record {
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
}

.left_record view:first-child {
  font-size: 32rpx;
  font-family: PingFang SC;
  /*font-weight: 500;*/
  color: #333333;
}

.left_record view:last-child {
  font-size: 24rpx;
  font-family: PingFang SC;
  /* font-weight: 500; */
  color: #333333;
  opacity: 0.6;
}

.right_record {
  font-size: 26rpx;
  font-family: PingFang SC;
  /* font-weight: 500; */
  color: #B6B9BE;
}

.right_record .give_icon {
  width: 42rpx;
  height: 42rpx;
  vertical-align: middle;
}

.tip_message {
  padding: 100rpx 0;
  text-align: center;
  font-size: 28rpx;
  color: #999999;
}

/* 没有更多 */
.no_more {
  padding: 30rpx;
  font-size: 28rpx;
  color: #999999;
  /* font-weight: 500; */
  text-align: center;
}