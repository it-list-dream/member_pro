.header {
  width: 100%;
  height: 370rpx;
  padding-top: 30rpx;
  box-sizing: border-box;
}

.header_content {
  position: relative;
  width: 690rpx;
  height: 340rpx;
  margin: 0 auto;
  font-family: PingFang SC;
  /* font-weight: 500; */
}

.header_content image {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 10;
}

.useInteBox {
  position: relative;
  width: 100%;
  height: 100%;
  overflow: hidden;
  box-sizing: border-box;
  z-index: 100;
  padding: 40rpx 50rpx;
}

.use_title {
  display: flex;
  justify-content: flex-end;
  align-items: center;
  color: #ffff;
}

.use_title text {
  margin-right: 136rpx;
}

.inteRules {
  font-size: 24rpx;
  font-family: PingFang SC;
  /* font-weight: 500; */
  color: #DCA66D;
}

.actScore {
  margin-top: 60rpx;
  font-size: 30rpx;
  font-family: PingFang SC;
  /* font-weight: 500; */
  color: #FFFFFF
}

.actScore text {
  color: #B6B9BE;
}

.vipScore {
  margin-top: 60rpx;
  display: flex;
  justify-content: space-between;
  font-size: 30rpx;
  font-family: PingFang SC;
  /* font-weight: 500; */
  color: #FFFFFF;
}
.vipScore>view{
  align-self: flex-end;
}
.vipScore text {
  color: #F4C08D;
}

.immediate {
  width: 140rpx;
  height: 50rpx;
  text-align: center;
  line-height: 50rpx;
  background: #DCA66D;
  border-radius: 30rpx;
  font-size: 24rpx;
  color: #1A1C20;
  flex-shrink: 0;
} 
