.useInte {
  /* margin-top: 20rpx;
  width: auto; */
  /* padding-top: 40rpx; */
  width: 100%;
  overflow: hidden;
  height: 340rpx;
  background: #F0F2F5;
}

.inte_item {
  overflow: hidden;
  margin: 0 auto;
  margin-top: 20rpx;
  width: 690rpx;
  height: 340rpx;
  background-size: 100% 100%;
  background-image: url(data:image/jpg;base64,)
}

/*  */
.title {
  padding-top: 30rpx;
  text-align: center;
  font-size: 30rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #FFFFFF;
}

.title text {
  padding-left:120rpx;
  font-size: 35rpx;
}

.rules {
  float: right;
  width: 130rpx;
  height: 50rpx;
  line-height: 50rpx;
  background: #7B6F85;
  border-radius: 25rpx 0px 0rpx 25rpx;
  font-size: 24rpx;
  font-family: PingFang SC;
  color: #FFFFFF;

}

.inte_detail {
  display: flex;
  /* margin-top: -28rpx; */
  flex-direction: column;
  justify-content: space-around;
  box-sizing: border-box;
  padding: 50rpx 30rpx 0 50rpx;
  width: 100%;
}

.inte_detail>view:first-child {
  font-size: 30rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #FFFFFF;
  line-height: 80rpx;
}

.inte_bot>view:first-child {
  margin-top: 24rpx;
  font-size: 30rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #FFFFFF;
}

.fen {
  color: #F4C08D;
}

.immediate {
  width: 160rpx;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  font-size: 30rpx;
  font-family: PingFang SC;
  color: #52495B;
  background-color: #ffff;
  border-radius: 30rpx;

}

.inte_bot {
  display: flex;
  justify-content: space-between;
}

/* 内容区 */
.container {
  padding: 0 30rpx;
  background: #ffff;
}

/*  */
.inte_tabs {
  position: relative;
  width: 100%;
  height: 100rpx;
  font-size: 32rpx;
  font-family: PingFang SC;
  color: #333333;
  /* background: purple; */
}

.tabs_item {
  float: left;
  width: 25%;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
}

.tabs_item:nth-child(-n+3) {
  position: relative;
}

.active::after {
  position: absolute;
  bottom: 12rpx;
  left: 50%;
  transform: translateX(-50%);
  content: '';
  width: 20rpx;
  height: 4rpx;
  background: #52495B;
  border-radius: 2rpx;

}

.tabs_item:last-child {
  position: relative;
  line-height: 0;
}

/*  */
.record {
  margin-top: 10rpx;
}

.record_item {
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 98rpx;
  border-bottom: 1px solid #eeeeee;
}

.left_record {
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
}

.left_record view:first-child {
  font-size: 32rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #333333;
}

.left_record view:last-child {
  font-size: 24rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #333333;
  opacity: 0.6;
}

.right_record {
  font-size: 26rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #B6B9BE;
}