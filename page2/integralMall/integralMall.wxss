/*  */
.pitch {
  display: flex;
  justify-content: space-between;
  height: 90rpx;
  margin: 20rpx 0 0 0;
  padding: 0 32rpx;
  background-color: #ffffff;
}

.pitch view:first-child {
  font-size: 36rpx;
  line-height: 90rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #333333;

}

.buy_count {
  display: flex;
  font-size: 38rpx;
  font-family: PingFang SC;
  height: 100%;
  align-items: center;
  text-align: center;
}

.buy_count>text:nth-child(2) {
  padding: 0 16rpx;
  font-family: PingFang SC;
}

.buy_count .symbol {
  width: 60rpx;
  height: 60rpx;
  line-height: 60rpx;
  background: #F0F2F5;
  font-size: 50rpx;
  font-family: PingFang SC;
  font-weight: 500;
}

.buy_count .symbol:first-child {
  border-radius: 8rpx 0 0 rpx;
}

.buy_count .symbol:last-child {
  border-radius: 0 8rpx 8rpx 0;
}

.buy_count text:nth-child(2) {
  margin: 0 1px;
  width: auto;
  height: 60rpx;
  padding: 0 20rpx;
  font-size: 40rpx;
  font-family: PingFang SC;
  font-weight: 500;
  background: #F0F2F5;
}

