}

.conternt {
  margin-bottom: 131rpx;
  width: 100%;
  height: 362rpx;
  background-color: #ffffff;
}

.w-h {
  width: 100%;
  height: 90rpx;
  line-height: 90rpx;
  padding: 0 32rpx;
}

.w-h:nth-child(n+2) {
  border-top: 1px solid #EEEEEE;
}

.integral {
  display: flex;
  align-items: center;
  font-size: 30rpx;
  font-family: PingFang SC;
  color: #333333;
}

.vip_inte {
  margin-right: 8rpx;
  display: block;
  width: 51rpx;
  height: 51rpx;
