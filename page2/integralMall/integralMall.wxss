.radio-group {
  display: flex;
}

.radio:last-child {
  margin-left: 48rpx;
}

/* radio */
radio .wx-radio-input.wx-radio-input-checked {
  border: none;
  background: #52495B !important;
}

/* 选中后的 对勾样式 （白色对勾 可根据UI需求自己修改） */
radio .wx-radio-input.wx-radio-input-checked::before {
  /* 去除对号 */
  content: '';
  width: 36rpx;
  height: 36rpx;
  border-radius: 50%;
  background-color: #52495B;
}