/* navbar */
/* @import '../../template/annimation/annimation.wxss'; */
.nav-wrap {
  position: fixed;
  width: 100%;
  top: 0;
  left: 0;
  z-index: 999;
}

.navbar {
  position: relative;
  top: 0;
  left: 0;
  z-index: 10;
  width: 100%;
  overflow: hidden;
  flex-shrink: 0;
  background: #ffffff;
}

.icon .icon-arrow {
  width: 36rpx;
  height: 36rpx;
}

.navbar-title {
  width: 100%;
  box-sizing: border-box;
  padding-left: 115px;
  padding-right: 115px;
  height: 32px;
  line-height: 32px;
  text-align: center;
  position: absolute;
  left: 0;
  z-index: 10;
  color: #333;
  font-size: 16px;
  font-weight: bold;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
}

.navbar-action-wrap {
  overflow: hidden;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -ms-flex-align: center;
  -webkit-align-items: center;
  align-items: center;
  position: absolute;
  left: 10px;
  z-index: 11;
  line-height: 1;
  padding-top: 4px;
  padding-bottom: 4px;
}


.navbar-action_item {
  padding: 3px 0;
  color: #333;
}

.navbar-action-group .navbar-action_item {
  border-right: 1px solid #f0f0f0;
  padding: 3px 14px;
}

.navbar-action-group .last {
  border-right: none;
}

.arrow {
  width: 36rpx;
  height: 36rpx;
  vertical-align: middle;
}

/*  */
.code_wrapper {
  display: flex;
  justify-content: center;
  align-items: center;
}

.code {
  width: 180rpx;
  height: 80rpx;
  text-align: center;
  line-height: 80rpx;
  background: #fff;
  font-size: 28rpx;
  color: #666666;
}

.qrcode {
  margin-top: 140rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  width: 100%;
}

.canvas_code {
  width: 160px;
  height: 160px;
}

.member_code {
  border-radius: 8rpx 0 0 8rpx;
  border: 1px solid #cccccc;
  border-right: 0 !important;
}

.unlocking {
  border-radius: 0 8rpx 8rpx 0;
  border: 1px solid #cccccc;
}

.active_code {
  color: #12D58B;
}

/*  */
.pull {
  position: absolute;
  left: 0;
  width: 100%;
}

.all {
  position: relative;
  padding-top: 80rpx;
  min-height: 100%;
}

.header {
  position: absolute;
  top: 42rpx;
  left: 50%;
  transform: translateX(-50%);
  z-index: 100;
}

.header image {
  width: 114rpx;
  height: 114rpx;
  border-radius: 50%;
}

.member_list {
  text-align: center;
  overflow: hidden;
  margin: 0 auto;
  width: 620rpx;
  height: 882rpx;
  background: #FFFFFF;
  box-shadow: 0 0 20rpx 0 rgba(82, 73, 91, 0.03);
  border-radius: 3rpx;
}

.member_store {
  margin-top: 120rpx;
  font-size: 30rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #333333;
}

.member_card {
  margin-top: 38rpx;
  font-size: 26rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: #333333;
}

.eff_date {
  margin-top: 36rpx;
  font-size: 24rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: #12D58B;
}

.member_item {
  position: relative;
  margin-top: 40rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  width: 100%;
  height: 160px;
}

.member_item>image {
  width: 320rpx;
  height: 320rpx;
}

.member_item::after {
  position: absolute;
  bottom: -38rpx;
  left: 50%;
  transform: translateX(-50%);
  display: block;
  content: '';
  width: 380rpx;
  height: 1px;
  background: #333333;

}

.canvas {
  background: #fff;
  width: 160px;
  height: 160px;
}

.year_card {
  margin-top: 76rpx;
  font-size: 30rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #333333;
}

.use_count {
  margin-top: 20rpx;
  font-size: 24rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: #333333;
}

.model {
  position: fixed;
  width: 100%;
  height: 100%;
  background: rgba(43, 44, 44, 0.7);
  /* top: 8%; */
  left: 0;
  z-index: 1000;
}

.modelcon {
  width: 100%;
  background: #EAECEF;
}

.close {
  height: 100%;
  width: 100%;
}

.modelcon .cardinfo {
  display: flex;
  font-size: 36rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #333333;
}

.store {
  font-size: 28rpx;
  color: rgba(0, 0, 0, 0.6);
}

.cardinfo text {
  padding-left: 20rpx;
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #333333;
}

.mask_item {
  padding: 0 30rpx;
  display: flex;
  height: 114rpx;
  align-items: center;
  justify-content: space-between;
}

.line {
  margin: 0 auto;
  width: 690px;
  height: 1px;
  background: #D9D9D9;
}

.cardStatus {
  align-self: flex-end;
  margin-left: 20rpx;
  font-size: 26rpx;
}

.notActive {
  margin: 38rpx;
  font-size: 24rpx;
  font-family: PingFangSC-Medium;
  font-weight: 500;
  color: red;
}

.member_list>view {
  overflow: hidden;
  height: 100% !important;
  box-sizing: border-box;
}

.tip_message {
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  color: #999999;
  font-size: 28rpx;
}

.gui {
  display: flex;
  flex-direction: column;
  justify-content: center;
}