page {
  background-color: #FFFFFF;
  font-family: PingFangSC-Medium, PingFang SC;
}

.contract-title {
  text-align: center;
  font-weight: 500;
  color: rgba(102, 102, 102, 1);
}

.member-info{
  font-size: 30rpx;
  color: rgba(102, 102, 102, 1);
  padding: 0 40rpx;
}
.member-info>view:not(:last-child){
  margin-bottom: 16rpx;
}

.contract-list {
  padding: 0 40rpx;
  font-size: 28rpx;
  line-height: 2;
  font-weight: 500;
  color: rgba(102, 102, 102, 1);
}
/*  */
.contractDesc {
  border-top: 10rpx solid #F6F9FF;
  padding: 30rpx 48rpx;
}

.member-list {
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.rules {
  width: 110rpx;
  height: 110rpx;
}

.handwrite {
  margin-left: 30rpx;
  width: 110rpx;
  height: 180rpx;
  transform: rotate(-90deg);
}

.member-list>view {
  display: flex;
  align-items: center;
  font-size: 28rpx;
  font-weight: 500;
  color: #333333;
  flex: 1;
}
.signing-date{
  line-height: 2em;
  font-size: 28rpx;
  color: #333333;
}

.signing{
  padding: 30rpx 0;
}

.signing .signBtn{
  width: 690rpx;
  height: 90rpx;
  margin: 0 auto;
  border-radius: 12rpx;
  line-height: 90rpx;
  text-align: center;
  font-weight: 500;
  background-color:#2564F6;
  color: #FFFFFF;
  font-size: 32rpx;
}

.multipleBtn{
  display: flex;
  justify-content: space-between;
  padding: 0 40rpx;
}
.multipleBtn .resigning{
  border: 1px solid #2564F6;
  color: #2564F6;
}
.multipleBtn .submitsigning{
  background-color: #2564F6;
  color: #FFFFFF;
}
.multipleBtn button{
  width: 286rpx;
  height: 86rpx;
  font-size: 32rpx;
  font-size: normal;
  line-height: 86rpx;
  text-align: center;
  margin: 0 !important;
  padding: 0 !important;
  border-radius: 12rpx;
}