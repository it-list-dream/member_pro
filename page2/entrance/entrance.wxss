/* page2/entrance/entrance.wxss */
.entrance {
  padding: 20rpx 30rpx 30rpx 30rpx;
  background-color: #F0F2F5;
}

.entrance_scroll {
  box-sizing: border-box;
  padding: 0 20rpx;
  width: 690rpx;
  background: #FFFFFF;
  border-radius: 8rpx;
}

.entrance_item {
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 69rpx;
  border-bottom: 1px solid #eeeeee;
}

.entrance_item>view:first-child {
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: 500;
}

.entrance_item>view:last-child {
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: 500;
}