.pickerdate {
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 124rpx;
  background-color: #ffffff;
  box-sizing: border-box;
  padding: 0px 28rpx 0px 28rpx;
}

.picker-list {
  display: flex;
}

.picker-list .picker-item {
  width: 234rpx;
  height: 64rpx;
  background: #F2F4F8;
  border-radius: 12rpx;
}



.picker {
  display: flex;
  height: 64rpx;
  justify-content: space-around;
  align-items: center;
  font-size: 26rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: #333333;
}

.picker image {
  width: 20rpx;
  height: 13rpx;
}

.picker-list .line {
  margin: 0 28rpx;
  font-size: 26px;
  font-weight: normal;
  color: #333333;
}

.pickerdate .tips {
  font-size: 26rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: #333333;
}

.power-list {
 padding-bottom: 30rpx;
 overflow: hidden;
}

.power-item {
  display: flex;
  align-items: center;
  width: 710rpx;
  height: 142rpx;
  background: #FFFFFF;
  border-radius: 4rpx;
  margin: 20rpx auto 0;
  padding: 0 20rpx;
  box-sizing: border-box;
}

.logo {
  width: 65rpx;
  height: 80rpx;
  margin: 20rpx;
}

.logo>image {
  width: 100%;
  height: 100%;
}

.power-data {
  display: flex;
  justify-content: space-between;
  flex: auto;
  font-size: 25rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #000000;
}
.power-data text:last-child{
  color: #B0B7C4;
}
.power-data-item{
  display: flex;
  align-items: center;
  flex-direction: column;
  justify-content: space-between;
}
.power-data-item>text:last-child{
  margin-top: 6rpx;
}