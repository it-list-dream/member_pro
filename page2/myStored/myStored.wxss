.stored_list {
  position: relative;
  width: 100%;
  height: 340rpx;
  background: #F0F2F5;
  overflow: hidden;
}

.stored_img {
  /* position: absolute;
  top: 20rpx;
  left: 50%;
  transform: translateX(-50%); */
  margin: 0 auto;
  margin-top:20rpx;
  overflow: hidden;
  width: 690rpx;
  height: 340rpx;
  /* background: #52495B; */
  background-size: 100% 100%;
  background-image: url(data:image/jpg;base64,);
  border-radius: 10rpx;
}

.stored_img_title {  
  font-size: 35rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #FFFFFF;
}

.stored_img view:first-child {
  padding-top: 30rpx;
}

.stored_img view {
  text-align: center;
}
.recharge{
  margin-top: 60rpx;
  font-size: 30rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #FFFFFF;
} 
.recharge text {
  color: #F4C08D !important;
}
.give{
  margin-top: 40rpx;
  font-size: 30rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #FFFFFF;
}
.give text {
  color: #12D58B;
}

.stored_tab {
  display: flex;
  height: 64rpx;
}

.stored_con {
  padding: 20rpx 20rpx 0 20rpx;
  box-sizing: border-box;
  /* height: auto; */
  background: #ffffff;
  
}

.tab {
  position: relative;
  flex: 1;
  font-size: 32rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: #333333;
  text-align: center;
  line-height: 64rpx;
}

.active::after {
  position: absolute;
  bottom: 0;
  left: 111rpx;
  content: '';
  width: 20rpx;
  height: 4rpx;
  background: #52495B;
  border-radius: 2rpx;
}

.storedWrapper {
  display: flex;
  height: 100rpx;
  justify-content: space-between;
  align-items: center;
  border-bottom: 1px solid #eeeeee;
}

.stored_item {
  width: 100%;
  padding-top: 30rpx; 
}
.stored_scroll{
  width: 100%;
  /* background: green; */
}
.stored_all {
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  height: 100%;
 
}

.stored_all view:first-child {
  font-size: 32rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #333333;
}

.stored_all view:last-child {
  font-size: 20rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #333333;
  opacity: 0.6;
}

.inte {
  font-size: 24rpx;
  font-family: PingFang SC;
  font-weight: 500;

}
