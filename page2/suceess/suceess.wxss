/* 预约成功 */
.success-box {
  display: flex;
  width: 100%;
  height: 60vh;
  align-items: center;
  justify-content: center;
}

.appoinment-succ,
.buy-succ,.inteConvert {
  display: flex;
  width: 100%;
  flex-direction: column;
  align-items: center;
}

.sucessTip {
  margin-top: 35rpx;
  font-size: 36rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: #333333;
}

.sucessBtn {
  margin-top: 102rpx;
  display: flex;
  width: 100%;
  justify-content: space-around;
}

button::after {
  border: none;
}

.btn {
  width: 300rpx !important;
  height: 62rpx;
  padding: 0;
  line-height: 62rpx;
  font-size: 30rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #333333;
  border-radius: 31rpx;
  text-align: center;
}

.cancel {
  border: 1px solid #333333;
}

.complete {
  line-height: 62rpx;
  background: #12D58B;
  color: #FFFEFE;
}

/* 购买成功 */
.buyTip {
  margin-top: 36rpx;
  font-size: 36rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: #333333;
}

.getInte {
  margin-top: 30rpx;
  font-size: 34rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #333333;
}

.getInte>text:last-child {
  color: #F4C08D;
}

/* 按钮的样式 */
.buyBtn {
  margin-top: 80rpx;
  display: flex;
  width: 100%;
  justify-content: space-around;
}

.buyCancel {
  background: #52495B;
  border: 0;
  color: #FFFFFF;
}

.buyComplete {
  padding: 0;
  background: #12D58B;
  line-height: 62rpx;
  color: #FFFEFE;
}

.comBox {
  margin-top: 80rpx;
}

.comBtn {
  width: 227rpx !important;
  height: 62rpx;
  line-height: 62rpx;
  padding: 0;
  border: 1px solid #333333;
  border-radius: 31rpx;
  font-size: 30rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #333333;
}
/* z */
.in{
  background: #12D58B;
  color: #FFFEFE;
}